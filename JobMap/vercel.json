{
  "crons": [
    {
      "path": "/api/cron/import-adzuna",
      "schedule": "0 */6 * * *"
    }
  ],
  "headers": [
    {
      "source": "/_next/static/:path*",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/api/:path*",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ],
  "trailingSlash": false
}